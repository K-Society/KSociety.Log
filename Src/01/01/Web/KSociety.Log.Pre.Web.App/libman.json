{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "vue@2.6.14",
      "destination": "wwwroot/lib/vue/",
      "files": [
        "dist/vue.min.js",
        "dist/vue.js",
        "LICENSE"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "lodash.js@4.17.21",
      "destination": "wwwroot/lib/lodash.js/",
      "files": [
        "lodash.min.js",
        "lodash.js"
      ]
    }
  ]
}